CREATE TABLE  public.students (
	"_id" serial NOT NULL,
	"first_name" varchar NOT NULL,
	"last_name" varchar NOT NULL,
	"school" varchar NOT NULL,
	PRIMARY KEY ("_id")
) WITH (
  OIDS=FALSE
);

